<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.joxxxx.footscapes.MenuUpcoming">


    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="32dp"
            android:layout_marginTop="31dp"
            android:text="UPCOMING"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Date"
            android:textSize="18dp"
            android:layout_below="@+id/textView32"
            android:layout_alignStart="@+id/textView32"
            android:layout_marginTop="25dp" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date"
            android:layout_marginStart="11dp"
            android:layout_alignBaseline="@+id/textView33"
            android:layout_alignBottom="@+id/textView33"
            android:layout_toEndOf="@+id/textView33" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView33"
            android:layout_marginStart="15dp"
            android:textSize="18dp"
            android:layout_toEndOf="@+id/editText3"
            android:text="to" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:ems="10"
            android:inputType="date"
            android:layout_alignBaseline="@+id/textView34"
            android:layout_alignBottom="@+id/textView34"
            android:layout_toEndOf="@+id/textView34" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginTop="23dp"
            app:srcCompat="@drawable/upcoming"
            android:layout_below="@+id/editText3"
            android:layout_alignEnd="@+id/editText3" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:text="Bantayan Island, Cebu"
            android:textSize="15dp"
            android:layout_alignTop="@+id/imageView2"
            android:layout_alignStart="@+id/textView34" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView33"
            android:layout_below="@+id/imageView2"
            android:layout_marginTop="15dp"
            android:text="BUCKET LIST" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="300dp"
            android:layout_height="150dp"
            android:layout_alignStart="@+id/textView37"
            android:layout_below="@+id/textView37"
            android:layout_marginTop="14dp"
            android:text="TextView" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
