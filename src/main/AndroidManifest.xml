<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joxxxx.bucketlisten">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appname"
        android:label="BucketListen"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".forgotpassword" />
        <activity android:name=".setpassword" />
        <activity android:name=".Menu" />
        <activity android:name=".SelfAwareness" />
        <activity android:name=".SelfAwareness2" />
        <activity android:name=".SelfAwareness3" />
        <activity android:name=".SelfAwareness4" />
        <activity android:name=".Register" />
        <activity android:name=".LoginController" />
        <activity android:name=".RegisterController" />
        <activity android:name=".verify"></activity>
    </application>

</manifest>